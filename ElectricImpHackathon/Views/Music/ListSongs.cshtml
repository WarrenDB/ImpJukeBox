@model List<ElectricImpHackathon.Models.SongTrack>

@{
    Layout = null;
}
<Response>
    <Message>
        @foreach(var song in Model)
        {
            song.ID + ": " + song.SongName + "\n";
        }
        Remember, send "PLAY " followed by the song ID to play that song!
    </Message>
</Response>